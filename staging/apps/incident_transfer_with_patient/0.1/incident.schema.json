        },
        "additionalInfo": "weitere Informationen",
        "gender": "Male",
        "dateOfBirth": "19801230",
        "age": 55,
        "healthInsuranceInformation": {
          "healthInsuranceCompany":"Musterkasse",
          "healthInsuranceCompanyNumber": 101234567,
          "insuranceNumber": "S123456789"
        },
        "initialAssessment": {
          "rmi": 360,
